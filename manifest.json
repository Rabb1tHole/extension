{

	"manifest_version": 2,
	"version": "0.1",
	"name": "Rabb1tHole",
	"description": "How'd you get down here?",
	"icons": { 
		"48": "icons/icon.svg",
		"96": "icons/icon.svg"
	},

	"permissions": [
		"tabs",
		"activeTab",
		"history",
		"storage"],

	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	
	"browser_action": {
		"default_icon": "icons/icon.svg",
		"default_title": "Rabb1tHole",
		"default_popup": "popup/popup.html",
		"browser_style": true
	},


	"options_ui": {
		"page": "options/options.html",
		"browser_style": true
	}

	
}